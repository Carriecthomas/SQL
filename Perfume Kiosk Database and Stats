-- Create a store database

CREATE TABLE perfume (id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER, price INTEGER);
INSERT INTO perfume VALUES (1, "Valentino", 5,140);
INSERT INTO perfume VALUES (2, "Carolina Herrera", 7,84);
INSERT INTO perfume VALUES (3, "Sol de Janeiro", 12,24);
INSERT INTO perfume VALUES (4, "Philosophy", 10, 50);
INSERT INTO perfume VALUES (5, "Zara",9,17);

SELECT * FROM perfume ORDER BY price;
SELECT MIN(quantity) FROM perfume

